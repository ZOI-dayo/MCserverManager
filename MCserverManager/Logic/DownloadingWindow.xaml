<Window x:Class="MCserverManager.Logic.DownloadingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MCserverManager.Logic"
        mc:Ignorable="d"
        Title="DownloadingWindow" Height="200" Width="500">
    <Grid x:Name="Main">
        <Label Content="ダウンロード中..." HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,25,0,0"/>
        <ProgressBar x:Name="MainBar" Height="20" VerticalAlignment="Center" Margin="10,0"/>

    </Grid>
</Window>
